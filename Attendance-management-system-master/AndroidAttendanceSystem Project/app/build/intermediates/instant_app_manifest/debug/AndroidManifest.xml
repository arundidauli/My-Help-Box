            android:label="@string/app_name" />
        <activity
            android:name="com.android.attendance.activity.AddAttandanceSessionActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.android.attendance.activity.AddAttendanceActivity"
            android:label="@string/app_name" />
